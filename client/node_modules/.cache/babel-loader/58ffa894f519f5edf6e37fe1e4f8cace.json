           </table>\n\n            <table style={{margin: \"30px 0px\"}}>\n                <thead>\n                    <tr>\n                        <th></th>\n                        <th>Products</th>\n                        <th>Quantity</th>\n                        <th>Price</th>\n                    </tr>\n                </thead>\n                <tbody>\n                    {\n                        orderDetails.cart.map(item =>(\n                        <tr key={item._id}>\n                            <td><img src={item.images.url} alt=\"\" /></td>\n                            <td>{item.title}</td>\n                            <td>{item.quantity}</td>\n                            <td>$ {item.price * item.quantity}</td>\n                        </tr>\n                        ))\n                    }\n                    \n                </tbody>\n            </table>\n        </div>\n    )\n}\n\nexport default OrderDetails\n"]},"metadata":{},"sourceType":"module"}